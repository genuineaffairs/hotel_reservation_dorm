<?xml version="1.0" ?>
<openerp>
<data>
	<!-- Reservation form view -->
	<record model="ir.ui.view" id="view_hotel_reservation_form_dorm">
	<field name="name">hotel.reservation.form</field>
	<field name="model">hotel.reservation</field>
	<field name="inherit_id" ref="hotel_reservation.view_hotel_reservation_form" />
	<field name="arch" type="xml">
		<xpath expr="//field[@name='dummy']" position="before">
		</xpath>
		<xpath expr="//tree[@string='Reservation Line']" position="inside">
			<newline />
			<field name="bed_reserve" string="Beds" />
		</xpath>
	</field>
	</record>
		
</data>
</openerp>
